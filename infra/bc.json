{
    "apiVersion": "v1",
    "kind": "BuildConfig",
    "metadata": {
        "annotations": {
            "openshift.io/generated-by": "probot-branches"
        },
        "labels": {
            "app": "#{app-name}"
        },
        "name": "#{app-name}-#{branch-name}",
        "namespace": "#{project}"
      },
    "spec": {
        "nodeSelector": null,
        "output": {
            "to": {
                "kind": "ImageStreamTag",
                "name": "#{app-name}-#{branch-name}:latest"
            }
        },
        "postCommit": {},
        "resources": {},
        "runPolicy": "Serial",
        "source": {
            "git": {
                "ref": "#{git-branch}",
                "uri": "#{git-home}"
            },
            "type": "Git"
        },
        "strategy": {
            "sourceStrategy": {
                "from": {
                    "kind": "ImageStreamTag",
                    "name": "nodejs:6",
                    "namespace": "openshift"
                }
            },
            "type": "Source"
        },
        "triggers": [
            {
                "generic": {
                    "secret": ""
                },
                "type": "Generic"
            },
            {
                "github": {
                    "secret": ""
                },
                "type": "GitHub"
            },
            {
                "type": "ImageChange"
            },
            {
                "type": "ConfigChange"
            }
        ]
    },
    "status": {
      "lastVersion":0
    }
}
