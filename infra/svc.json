{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "labels": {
            "app": "#{app-name}"
        },
        "name": "#{app-name}-#{branch-name}",
        "namespace": "#{project}"
    },
    "spec": {
        "clusterIP": "#{server-ip}",
        "ports": [
            {
                "name": "3000-tcp",
                "port": 8080,
                "protocol": "TCP",
                "targetPort": 3000
            }
        ],
        "selector": {
            "deploymentconfig": "#{app-name}-#{branch-name}"
        },
        "sessionAffinity": "None",
        "type": "ClusterIP"
    },
    "status": {
        "loadBalancer": {}
    }
}
